{
  "name": "airy-canvas",
  "version": "0.1.1",
  "description": "The realization of a front-end drawing board.",
  "private": false,
  "main": "index.js",
  "scripts": {
    "dist": "vue-cli-service build --target lib --name index ./src/index.js",
    "serve": "vue-cli-service serve",
    "lint": "vue-cli-service lint"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "canvas"
  ],
  "dependencies": {
    "@types/vue": "^2.0.0",
    "vue": "^2.6.6"
  },
  "devDependencies": {
    "@types/eslint": "^4.16.6",
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-service": "^3.5.1",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "eslintIgnore": [
    "dist/*"
  ],
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
